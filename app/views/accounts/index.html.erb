<div id="logo">
  <%= image_tag("small_logo.png") %>
</div>
<br><br>

<%= form_tag sessions_path, id: 'account-login' do %>
    <%= text_field_tag :username, '', :placeholder => "Username" %>
    <%= password_field_tag :password,'', :placeholder => "Password" %>
    <%= submit_tag "Log In" , :class => "submit", data: {type: 'submit'} %>
<% end %>
<div class="signup">
<%= link_to "Sign Up", new_account_path, :data => { type: 'signup' }, :remote => true %>
</div>
<br><br>






