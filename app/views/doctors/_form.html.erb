<%= form_for [diabetic, doctor], remote: true do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name, :required => true %><br><br>
  <%= f.label :fax %><br>
  <%= f.text_field :fax, :required => true %><br><br>
  <%= f.label :email %><br>
  <%= f.email_field :email %><br><br>
  <%= f.label :comments %><br>
  <%= f.text_area :comments %><br><br>
<% end %>
