<% if diabetic %>
  <%= button_tag "Add a record for: #{diabetic.name}", data: { type: 'dashboard-button', url: new_diabetic_record_path(diabetic) }, remote: true %><br>
  <%= button_tag "View records for #{diabetic.name}", data: { type: 'dashboard-button', url: diabetic_records_path(diabetic) }, remote: true %><br>
  <%= button_tag "View Graph for #{diabetic.name}", data: { type: 'graph-button', url: show_graph_path(account,diabetic) }, remote: true %><br>
  <%= button_tag "Edit #{diabetic.name}'s info", data: { type: 'dashboard-button', url: diabetic_dashboard_path(diabetic.id) }, remote: true %><br>
<% else %>
  <%= button_tag 'Edit the account', data: { type: 'dashboard-button', url: edit_account_path(account) }, remote: true %><br>
<% end %>
<%= button_tag 'Add a Diabetic', data: { type: 'dashboard-button', url: new_account_diabetic_path(account) }, remote: true %><br>




<div id="container" style="min-width: 310px; height: 400px; margin: 0 auto"></div>